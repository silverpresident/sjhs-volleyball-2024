{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=VolleyballRally;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Database": {
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  },
  "AppSettings": {
    "SignalR": {
      "HubUrl": "/TournamentHub"
    }
  },
  "Authentication": {
